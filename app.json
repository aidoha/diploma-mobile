{
    "expo": {
        "name": "diploma-mobile",
        "slug": "diploma-mobile",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "splash": {
            "image": "./assets/images/group.png",
            "resizeMode": "cover"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}